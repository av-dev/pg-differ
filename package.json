{
  "name": "pg-differ",
  "version": "1.1.1",
  "description": "Node.js module for easy synchronization of postgres tables with models (json-schemas), alternative migration",
  "main": "src/index.js",
  "types": "types/index.d.ts",
  "keywords": [
    "postgres",
    "node.js"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "test": "mocha test/**/*.test.js",
    "eslint": "eslint --max-warnings=0 **/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/av-dev/pg-differ.git"
  },
  "bin": {
    "pg-differ": "./bin/pg-differ"
  },
  "author": "Andrey Vereshchak",
  "license": "MIT",
  "dependencies": {
    "ajv": "^6.10.0",
    "chalk": "^2.4.2",
    "minimist": "^1.2.0",
    "pg": "^7.8.2",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "eslint": "^5.15.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1",
    "mocha": "^5.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "concurrently \"npm run test\" \"npm run eslint\""
    }
  }
}
