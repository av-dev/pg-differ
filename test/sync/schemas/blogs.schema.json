{
  "table": "${schema}.blogs",
  "forceIndexes": [
    "index",
    "foreignKey"
  ],
  "indexes": [
    {
      "type": "foreignKey",
      "references": {
        "table": "users",
        "columns": [
          "id"
        ]
      },
      "match": "FULL",
      "onDelete": "CASCADE",
      "columns": [
        "maker"
      ]
    }
  ],
  "columns": [
    {
      "name": "id",
      "type": "bigint",
      "primaryKey": true
    },
    {
      "name": "maker",
      "type": "bigint"
    },
    {
      "name": "body",
      "type": "json",
      "default": {
        "2": "some value",
        "1": "some value"
      }
    },
    {
      "name": "description",
      "type": "varchar(255)"
    },
    {
      "name": "created",
      "type": "timestamp"
    }
  ]
}
