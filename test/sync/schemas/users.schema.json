{
  "table": "users",
  "forceIndexes": [
    "unique",
    "foreignKey"
  ],
  "indexes": [
    {
      "type": "unique",
      "columns": [
        "id",
        "code"
      ]
    }
  ],
  "columns": [
    {
      "name": "id",
      "type": "bigint",
      "unique": true,
      "primaryKey": true
    },
    {
      "name": "age",
      "type": "bigint",
      "default": "18"
    },
    {
      "name": "parent",
      "type": "integer",
      "unique": true
    },
    {
      "name": "code",
      "type": "varchar(255)"
    },
    {
      "name": "description",
      "type": "varchar(255)"
    }
  ]
}
